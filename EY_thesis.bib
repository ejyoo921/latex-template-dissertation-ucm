	note = {Publisher: Royal Society},
	pages = {260--281},
	file = {Full Text PDF:/Users/eunji/Zotero/storage/3FKRJET9/Rivlin and Rideal - 1997 - The hydrodynamics of non-Newtonian fluids. I.pdf:application/pdf},
}

