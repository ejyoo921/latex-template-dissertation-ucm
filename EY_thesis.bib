	journal = {Journal of Computational Physics},
	author = {Ying, Lexing and Biros, George and Zorin, Denis},
	month = may,
	year = {2004},
	keywords = {-body problems, Double-layer potential, Fast multipole methods, Fast solvers, Integral equations, Particle methods, Single-layer potential},
	pages = {591--626},
	file = {ScienceDirect Full Text PDF:/Users/eunji/Zotero/storage/P3WID2WI/Ying et al. - 2004 - A kernel-independent adaptive fast multipole algor.pdf:application/pdf;ScienceDirect Full Text PDF:/Users/eunji/Zotero/storage/Y2VRC5HG/Ying et al. - 2004 - A kernel-independent adaptive fast multipole algor.pdf:application/pdf},
}

